<h1>Listing customer_sessions</h1>

<table>
  <tr>
    <th>Customer</th>
    <th>Browser</th>
    <th>Platform</th>
    <th>Browser version</th>
    <th>Referer link</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @customer_sessions.each do |customer_session| %>
  <tr>
    <td><%= customer_session.customer %></td>
    <td><%= customer_session.browser %></td>
    <td><%= customer_session.platform %></td>
    <td><%= customer_session.browser_version %></td>
    <td><%= customer_session.referer_link %></td>
    <td><%= link_to 'Show', customer_session %></td>
    <td><%= link_to 'Edit', edit_customer_session_path(customer_session) %></td>
    <td><%= link_to 'Destroy', customer_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

