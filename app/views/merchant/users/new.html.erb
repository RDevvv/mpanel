<div class="row-fluid">
	<%= form_for([@account,@user], :url=>merchant_account_users_path(@account,@user)) do |f| %>
		<h4>Create New User</h4>
		<div class="span9">
			<%= f.collection_select :id, @users,:id,:user_key%>
		</div>
		<div class="clearfix"></div>
		<div class="span9">
			<%= f.text_field :first_name, :placeholder=>"First Name", :class=>"span4"%>
			<%= f.text_field :last_name, :placeholder=>"Last Name", :class=>"span4"%>
		</div>
		<div class="clearfix"></div>
		<div class="span9">
			<%= f.text_field :email, :placeholder=>"Email", :class=>"span8"%>
		</div>
		<div class="clearfix"></div>
		<div class="span9">
			<%= f.submit "Add User", :class=>"btn btn-primary span3"%>
		</div>
		<div class="clearfix"></div>
	<% end %>
</div>