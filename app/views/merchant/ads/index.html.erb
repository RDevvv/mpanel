<%= render :partial=>'header' %>
<div class="pull-right">
  <%= link_to "Create Ad",new_merchant_account_account_brand_ad_path(@current_account,@account_brand),:class=>"btn btn-info"%>
  <br/>
  <br/>  
</div>
<div class="clearfix"></div>
<div class="box">
  <div class="box-header">
    <ul class="nav nav-tabs nav-tabs-left">
      <li class="active"><a href="#home" data-toggle="tab"><i class="icon-home"></i></a></li>
      <li><a href="#expiry_ads" data-toggle="tab"><i class="icon-comments-alt"></i> <span>Expiry Ads</span></a></li>
    </ul>
    <div class="title">Left tabs</div>
  </div>

  <div class="box-content">
    <div class="tab-content">
      <div class="tab-pane active" id="home">
        <%= render :partial=>"ads",:locals=>{:ads=>@ads} %>
      </div>
      <div class="tab-pane" id="expiry_ads">
        <%= render :partial=>"ads",:locals=>{:ads=>@expiry_ads} %>
      </div>
    </div>
  </div>
</div>
  
