<head>
<meta charset="utf-8">
<title>GullakMaster - Mapview</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">

<link href="css/transition.css" rel="stylesheet" type="text/css"/>
<script type="text/javascript" src="js/fasw.transitions.min.js"></script>
<script type="text/javascript">
(function inittrans() 
 {
 var params =  //All params are optional, you can just assign {} 
 { 
 "navB" : "slide reverse", //Effect for navigation button, leave it empty to disable it
 "but" : true,       //Flag to enable transitions on button, false by default
 "cBa" : onTransitionFinished // function() { alert("Done!"); }  //callback function
 };
 new ft(params);
 })();

function onTransitionFinished()
{
    //Do anything you want 
}

</script>
<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
<!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]
    <script type="text/javascript" src="http://code.jquery.com/jquery-1.5.min.js"></script>
    <script type="text/javascript" src="http://code.jquery.com/mobile/1.0a4.1/jquery.mobile-1.0a4.1.min.js"></script>-->
<!-- Preloader -->
<script type="text/javascript">// <![CDATA[
$(window).load(function() { $("#spinner").fadeOut("slow"); })
// ]]>
</script>
</head>
<body>
<!-- Preloader -->
<div id="spinner"></div>
<img src="img/ajax-loader.gif" id="loading-indicator" style="display:none" />
<header>
    <div class="back-btn"><a href="outletview-headertest1.html" class="back-btn2" data-ftrans="slide">&nbsp;</a></div>
    <div class="head-brand">Ice-cream</div>
</header>

<div class="container-location">
    <div class="row-fluid">
        <div class="span12">
            <iframe id="map_canvas" width="100%" height="209" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?f=q&amp;spn=0.36664,0.676346&amp;t=m&amp;z=11&amp;iwloc=1&amp;cid=4670488372095120642&amp;output=embed"></iframe>
        </div>
        <div class="ml0 span12">
            <div class="out-add-div">
                <% if @outlet_versions.nil? %>
                    No results found
            <% else %>
                    <% @outlet_versions.each do |outlet_version| %>
                    <% if outlet_version.is_active == true %>
                    <% puts outlet_version.id %>
                <div class="bdiv">
                    <div class="bbox">
                        <div class="brandtitle">maple</div>
                        <div class="eye2"><a onclick="$.pnotify({
                                title: 'Offer',
                                text: 'The offer text would get displayed here.',
                                opacity: .9
                                });" class="btn source"></a></div>
                        <div align="center" class="brandlogo"><img src="img/maple-medium.png" border="0" /> </div>
                        <div class="rch-btns-div">
                            <div class="btn190"><a class="button_call2" href="tel:+91-<%= outlet_version.outlet.mobile_number %>">CALL</a></div>
                        </div>
                        <div class="distance-div"><%= (outlet_version.distance * 1.6).to_f.round(2) %> km</div>
                    </div>
                    <div class="lower-box">
                        <div class="refr2"></div>
                        <div class="star2"></div>
                        <div class="right-box">
                            <a href="#">
                                <div class="run2"></div>
                                <div class="runtxt">20.3K</div>
                            </a>
                        </div>
                    </div>
                </div>
            <% end %>
            <% end %>
            <% end %>
            </div>
        </div>
    </div>
</div>
</body>
