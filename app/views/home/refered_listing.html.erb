<div align="center" class="container-location">
    <div class="row-fluid">
        <div class="span12">
            <% if @outlet_versions.nil? %>
                No results found
            <% else %>
                <% @outlet_versions.each do |outlet_version| %>
                <% if outlet_version.is_active == true %>
            <div class="bdiv">
                <div class="bbox">
                        <% brand = outlet_version.outlet.account_brand.brand %>
                        <div class="brandtitle"><%= brand.brand_name[0..6] %>
                        <% unless brand.brand_name.length < 7 %>
                                ..
                            <% end %>
                            </div>
                    <div class="eye2"><a onclick="$.pnotify({
                            title: 'Offer',
                            text: 'The offer text would get displayed here.',
                            opacity: .9
                            });" class="btn source"></a></div>
                    <div align="center" class="brandlogo">
                    <% if brand.attachments.empty? %>
                        <%= image_tag "assets/categories/#{brand.category.category_name}.png", :class => "img-responsive" %>
                    <% else %>
                            <%= image_tag brand.attachments.first.image, :class => "img-responsive" %>
                    <% end %>
                    </div>
                    <div class="rch-btns-div">
                        <div class="btn190"><a class="button_map2" onclick="track(this)" href="#">ADDRESS</a></div>
                        <% unless outlet_version.outlet.mobile_number.empty? %>
                            <div class="btn190">
                            <a class="button_call2" onclick="track(this)" href="tel:+91-<%= outlet_version.outlet.mobile_number %>">CALL</a>
                        </div>
                    <% end %>
                    </div>
                    <div class="distance-div">Distance: <%= (outlet_version.distance * 1.6).to_f.round(2) %> km</div>
                    <div class="lower-box">
                        <a href="https://www.facebook.com/sharer/sharer.php?u=http://gullakmaster.com" target="_blank"><div class="refr2"></div></a>
                        <div class="star2"></div>
                        <div class="right-box">
                            <a href="#">
                                <div class="run2"></div>
                                <div class="runtxt">20.3K</div>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        <% end %>
        <% end %>
        <% end %>
        </div>
    </div>

