<div class="container-location">
    <div class="row-fluid">
        <%= render 'top_location_search_bar' %>
        <div>
            <% if @final_outlets.nil? %>
                No results found
            <% else %>
                <% @final_outlets.each do |outlet| %>
                    <% outlet.ads.each do |ad| %>
                            <%= render :partial => 'poster', :locals => {:outlet => outlet, :ad => ad} %>
                    <% end %>
                <% end %>
                <%#= paginate @final_outlets %>
            <% end %>
        </div>
    </div>
</div>
<%= render :partial => "back_to_top" %>
<%= render :partial => "new_mobile_number_modal" %>
<%= render :partial => "verify_mobile_number" %>
<%= render :partial => "share_partial" %>


<script type="text/javascript">
modal_submit();
verification_modal_submit();
get_backtotop_button();
</script>
