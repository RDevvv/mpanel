<div id="active_admin_content" class="without_sidebar">
  <div id="main_content_wrapper">
    <div id="main_content">
      <div class="blank_slate_container" id="dashboard_default_message">
        <%= form_tag(admin_button_click_chart_path, :method => "post") do %>
            <%= select_tag(:chart_type, options_for_select([['Call', 'call'], ['Address', 'address'], ['View', 'description']], selected: :option )) %>
            <%= select_month(Date.today) %>
            <%= select_year(Date.today, :start_year => 2013, :end_year => Date.today.year) %>
            <%= submit_tag "Submit" %>
        <% end %>
      </div>
    </div>
  </div>
</div>